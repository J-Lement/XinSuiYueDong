<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nchu.music.dao.LoginDao">

    <select id="checkByNameAndPassword" parameterType="String" resultType="com.nchu.music.bean.User">
          select * from user where user_name=#{name} and password=#{password}and status=1
    </select>


</mapper>